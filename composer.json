{
    "name": "cable8mm/qr-images",
    "description": "QR images generated by command",
    "keywords": [
        "qr",
        "qr code",
        "generated",
        "symfony console",
        "command"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.0|^7.0",
        "chillerlan/php-qrcode": "^5.0",
        "setasign/fpdf": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Cable8mm\\QrImages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cable8mm\\QrImages\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "lint": "./vendor/bin/pint",
        "inspect": "./vendor/bin/pint --test",
        "apidoc": "doctum.phar update doctum.php --output-format=github -v --no-ansi --no-progress",
        "app": "./bin/application"
    }
}